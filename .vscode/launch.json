{
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "server",
            "processId": "${command:PickProcess}",
            "restart": true,
            "protocol": "inspector",
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:9001",
            "webRoot": "${workspaceFolder}/frontend"
        }
    ],
    "compounds": [
        {
            "name": "Server + client",
            "configurations": [
                "server",
                "client",
            ]
        }
    ]
}